include "std.rasm"

println(test(5));
nListPrintln(test2(5));

fn test(n: i32) -> i32 {
    let ten = it(10);

    test1(ten, { v ->
        let five = it(5);

        add(n, add(v, five));
    });
}

fn test1(v: i32, f: fn(i32) -> i32) -> i32 {
    f(v);
}


fn test2(n: i32) -> List<i32> {
    let ten = listOf(10);
    listAppend(listOf(20), ten);
}