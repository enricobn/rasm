include "std.rasm"

enum Option<T> {
  None,
  Some(value : T)
}

OptionMatch(Option::Some("a value"), {sprintln("none");}, {sprintln("some");});
OptionMatch(Option::None(), {sprintln("none");}, {sprintln("some");});
OptionMatch(Option::Some("a value"), {sprintln("none");}, {sprintln("some");});
OptionMatch(Option::None(), {sprintln("none");}, {sprintln("some");});