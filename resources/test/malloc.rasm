include "std.rasm"

printOption(Option::Some("1"));
printOption(Option::Some("2"));
printOption(Option::Some("3"));
printOption(Option::Some("4"));
printOption(Option::Some("5"));

printAllocated();

fn printOption(o: Option<&str>) {
    Option::run(o, { v -> sprint("Some(");sprint(v);sprintln(")");}, { -> sprintln("None");});
}
