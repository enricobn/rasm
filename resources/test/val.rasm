include "std.rasm"

with(atoi(argv(1)), { arg ->
    nprintln(parameter(arg));
    nprintln(lambdaParameter(arg, { p -> p;}));
    nprintln(context(arg));
});

fn parameter(n: i32) -> i32 {
    n;
}

fn lambdaParameter(n: i32, f: fn(i32) -> i32) -> i32 {
    f(n);
}

fn context(n: i32) -> i32 {
    lambdaParameter(n, { n1 -> n;});
}