inline asm lessOrEqual(a: i32, b: i32) -> bool /{
    mov     eax,$a
    cmp     eax,$b
    mov     eax,1 ; true
    jbe     $+7  ; Jump if Below or Equal unsigned comparison
    mov     eax,0 ; false
}/

