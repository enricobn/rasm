enum Option<T> {
  Some(value : T),
  None
}

fn toList<T>(o: Option<T>) -> List<T> {
    match(o, { v -> listOf(v);}, { -> List::Empty();});
}

fn print<T>(o: Option<T>) {
    run(o, { v -> print("Some("); print(v); print(")");}, { -> print("None");});
}

fn getOrElse<T>(o: Option<T>, default: T) -> T {
    match(o, { v -> v;}, { -> default;});
}