pub native initRandom() /{
    $include(<time.h>)
    $include(<sys/time.h>)
    $include(<stdlib.h>)

    srand(time(NULL));   // Initialization, should only be called once.
}/

pub inline native random() -> i32 /{
    $include(<time.h>)
    $include(<sys/time.h>)
    $include(<stdlib.h>)

    return rand();
}/