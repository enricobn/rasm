pub native initRandom() /{
    $include(<time.h>)
    $include(<sys/time.h>)
    $include(<stdlib.h>)

    srand(time(NULL));   // Initialization, should only be called once.
}/

pub native random() -> int /{
    $include(<time.h>)
    $include(<sys/time.h>)
    $include(<stdlib.h>)
    $inline()

    return rand();
}/

