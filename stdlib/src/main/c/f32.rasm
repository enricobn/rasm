    ($a + $b)
}/

pub inline native mul(a: f32, b: f32) -> f32 /{
    ($a * $b)
}/

pub inline native div(a: f32, b: f32) -> f32 /{
    ($a / $b)
}/

pub inline native sub(a: f32, b: i32) -> f32 /{
    ($a - $b)
}/

pub inline native sub(a: i32, b: f32) -> f32 /{
    ($a - $b)
}/

pub inline native greater(a: f32, b: f32) -> bool /{
    ($a > $b)
}/

