pub inline native tof32(n: i32) -> f32 /{
    ((float) $n)
}/

pub inline native sub(a: f32, b: f32) -> f32 /{
    ($a - $b)
}/

pub inline native add(a: f32, b: f32) -> f32 /{
    ($a + $b)
}/

