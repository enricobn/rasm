pub struct Circle {
    x: i32,
    y: i32,
    radius: i32
}

pub fn Shape(circle: Circle) -> Shape {
    Shape(
        fn() { Point(circle.x, circle.y); },
        fn(dx, dy) {
            let result = Circle(circle.x.add(dx), circle.y.add(dy), circle.radius);

            Shape(result);
        }, fn() {
          //printRef(circle);
            "Circle("
                .add(circle.x)
                .add(",")
                .add(circle.y)
                .add(",")
                .add(circle.radius)
                .add(")");
    });
}