pub struct Circle {
    x: int,
    y: int,
    radius: int
}

pub fn Shape(circle: Circle) -> Shape {
    Shape(
        { Point(circle.x, circle.y); },
        fn(dx, dy) {
            let result = Circle(circle.x.add(dx), circle.y.add(dy), circle.radius);

            Shape(result);
        }, {
          //printRef(circle);
            "Circle("
                .append(circle.x)
                .append(",")
                .append(circle.y)
                .append(",")
                .append(circle.radius)
                .append(")");
    });
}