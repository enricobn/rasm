pub struct Point {
    x: int,
    y: int
}

pub fn toString(p: Point) -> str {
    "Point(".append(p.x).append(",").append(p.y).append(")");
}

pub fn Shape(point: Point) -> Shape {
    Shape(
        { point; },
        fn(dx, dy) {
            let result = Point(point.x.add(dx), point.y.add(dy));

            Shape(result);
        }, { toString(point); }
    );
}