struct Circle {
    x: i32,
    y: i32,
    radius: i32
}

fn ToString(circle: Circle) -> ToString {
    ToString({ -> "Circle"; });
}

fn Shape(circle: Circle) -> Shape {
    Shape(
        ToString(circle),
        { dx, dy ->
            let result = Circle(add(circle.x, dx), add(circle.y, dy), circle.radius);

            Shape(result);
        }, { ->
              //printRef(circle);
              print("Circle(", circle.x);
              print(",", circle.y);
              print(",", circle.radius);
              print(")");
         });
    }