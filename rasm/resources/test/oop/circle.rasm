struct Circle {
    x: i32,
    y: i32,
    radius: i32
}

fn Shape(circle: Circle) -> Shape {
    Shape(
        { -> Point(circle.x, circle.y); },
        { dx, dy ->
            let result = Circle(add(circle.x, dx), add(circle.y, dy), circle.radius);

            Shape(result);
        }, { ->
              //printRef(circle);
              add(add("Circle(", circle.x, ",", circle.y),
                add(",", circle.radius, ")"));
         });
    }