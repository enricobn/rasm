pub struct Shape {
    center: fn() -> Point,
    move: fn(int,int) -> Shape,
    toString: fn() -> str
}

pub fn toShape<T>(inner: T) -> Shape {
    Shape(
        { inner.center; },
        fn(dx, dy) { inner.move(dx, dy);},
        { inner.toString; }
    );
}

pub fn print(file: File, shape: Shape) -> Option<IOError> {
    print(file, shape.toString.call());
}