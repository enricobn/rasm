choose(atoi(argv(1)), fn() { println("0"); }, fn() { println("1"); }, fn() { println("2"); }, fn() { println("3"); });

fn choose(n: i32, l0: fn() -> (), l1: fn() -> (), l2: fn() -> (), l3: fn() -> ()) {
    if(eq(0, n),
        fn() { l0(); },
        fn() { if(eq(1, n),
                fn() { l1(); },
                fn() { if(eq(2,n),
                    fn() { l2(); },
                    fn() { l3(); }
                );});
        });
}